[user]
    name = Bryan Lee
    email = pygmize.me@gmail.com
[alias]
    graph = log --graph --format=\"%C(auto)%h %C(italic)%an%C(auto)%d %s\"
    pr = "!f() { git fetch upstream pull/\"$1\"/head:pr-\"$1\"; git checkout pr.\"$1\"; }; f"
[credential]
    helper = cache
[include]
    path = ~/.gitconfig_local
[core]
    excludesfile = ~/.gitignore_global
